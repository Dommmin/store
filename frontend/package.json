{
   "name": "frontend",
   "version": "0.1.0",
   "private": true,
   "scripts": {
      "dev": "next dev",
      "build": "next build",
      "start": "next start",
      "lint": "next lint",
      "lint:fix": "next lint --fix",
      "format": "prettier --write ./**/*.{js,jsx,ts,tsx,css,md,json} --config ./.prettierrc.json"
   },
   "dependencies": {
      "@ckeditor/ckeditor5-build-classic": "^41.3.1",
      "@ckeditor/ckeditor5-ckbox": "^41.3.1",
      "@ckeditor/ckeditor5-image": "^41.3.1",
      "@ckeditor/ckeditor5-react": "^6.3.0",
      "@ckeditor/ckeditor5-upload": "^41.3.1",
      "@headlessui/react": "^2.0.1",
      "@heroicons/react": "^2.1.3",
      "@radix-ui/themes": "^3.0.3",
      "@tanstack/react-query": "^5.32.0",
      "axios": "^0.21.1",
      "ckeditor5-custom-build": "^0.0.6",
      "class-variance-authority": "^0.7.0",
      "classnames": "^2.5.1",
      "clsx": "^2.1.1",
      "js-cookie": "^3.0.5",
      "laravel-echo": "^1.16.1",
      "lucide-react": "^0.378.0",
      "meilisearch": "^0.38.0",
      "next": "14.2.3",
      "prop-types": "^15.8.1",
      "pusher-js": "^8.4.0-rc2",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-multi-carousel": "^2.8.5",
      "react-range-slider": "^0.2.1",
      "react-range-slider-input": "^3.0.7",
      "react-slider": "^2.0.6",
      "react-toastify": "^10.0.5",
      "swr": "^2.2.5",
      "tailwind-merge": "^2.3.0",
      "tailwindcss-animate": "^1.0.7",
      "use-debounce": "^10.0.0"
   },
   "devDependencies": {
      "@eslint/js": "^9.2.0",
      "@next/eslint-plugin-next": "^10.0.4",
      "@tailwindcss/forms": "^0.5.2",
      "@types/node": "20.12.7",
      "@types/react": "18.2.79",
      "autoprefixer": "^10.4.2",
      "babel-eslint": "^10.1.0",
      "daisyui": "^4.11.1",
      "eslint": "^8.57.0",
      "eslint-config-next": "^14.2.3",
      "eslint-config-prettier": "^7.1.0",
      "eslint-plugin-prettier": "^3.3.0",
      "eslint-plugin-react": "^7.34.1",
      "framer-motion": "^11.1.9",
      "globals": "^15.2.0",
      "postcss": "^8",
      "prettier": "^3.2.5",
      "prettier-plugin-tailwindcss": "^0.5.14",
      "tailwindcss": "^3.4.1"
   },
   "eslintConfig": {
      "extends": [
         "next",
         "next/core-web-vitals"
      ],
      "rules": {
         "import/prefer-default-export": 0,
         "no-console": "warn",
         "no-nested-ternary": 0,
         "no-underscore-dangle": 0,
         "no-unused-expressions": [
            "error",
            {
               "allowTernary": true
            }
         ],
         "camelcase": 0,
         "react/self-closing-comp": 1,
         "react/jsx-filename-extension": [
            1,
            {
               "extensions": [
                  ".js",
                  "jsx"
               ]
            }
         ],
         "react/prop-types": 0,
         "react/destructuring-assignment": 0,
         "react/jsx-no-comment-textnodes": 0,
         "react/jsx-props-no-spreading": 0,
         "react/no-array-index-key": 0,
         "react/no-unescaped-entities": 0,
         "react/require-default-props": 0,
         "react/react-in-jsx-scope": 0,
         "linebreak-style": [
            "error",
            "unix"
         ],
         "semi": [
            "error",
            "always"
         ],
         "prettier/prettier": [
            "error",
            {
               "endOfLine": "auto"
            },
            {
               "usePrettierrc": true
            }
         ]
      }
   }
}
